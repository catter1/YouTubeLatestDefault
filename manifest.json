{
    "manifest_version": 3,
    "name": "YouTube Latest Default",
    "version": "1.0",
    "description": "Sets 'Latest' as the default sorting option on YouTube channel videos",
    "permissions": [
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://www.youtube.com/*/videos",
        "https://www.youtube.com/*/shorts"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon-16.png",
        "default_title": "YouTube Latest Default"
    },
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}